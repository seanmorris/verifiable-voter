{
	"name": "VerifiableVoter NYS",
	"version": "1.0",
	"description": "Ensure you're addressing a real, registered NYS voter on Facebook, and not just feeding the trolls.",
	"manifest_version": 2,
	"icons": {
		"16": "icons/16.png"
		, "48": "icons/48.png"
		, "128": "icons/128.png"
	}
	, "content_scripts": [{
		"matches": [
			"http://facebook.com/*"
			, "https://facebook.com/*"
			, "http://*.facebook.com/*"
			, "https://*.facebook.com/*"
		],
		"js": ["content.js"]
	}]
}